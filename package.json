{
  "name": "anime-kun-nestjs-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npx prisma generate",
    "build": "npx prisma generate && nest build && node build.js",
    "build:railway": "npx prisma generate && npx tsc",
    "build:vercel": "npx prisma generate && npm run build",
    "vercel-build": "npm run build:vercel",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "warm-cache": "node -r ts-node/register -r tsconfig-paths/register scripts/warmup-cache.ts",
    "fix-sequences": "npx prisma db execute --file ./prisma/fix-sequences.sql --schema ./prisma/schema.prisma",
    "cleanup-images": "ts-node -r tsconfig-paths/register scripts/cleanup-orphaned-images.ts",
    "cleanup-images:dry-run": "npm run cleanup-images -- --dry-run",
    "update-anime-popularity": "ts-node -r tsconfig-paths/register scripts/update-anime-popularity.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod:dev": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:ts": "node --max-old-space-size=1024 -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.964.0",
    "@keyv/redis": "^5.1.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "@sentry/nestjs": "^10.32.1",
    "@sentry/node": "^10.32.1",
    "@types/bcrypt": "^6.0.0",
    "@types/nodemailer": "^7.0.0",
    "@types/passport-google-oauth20": "^2.0.17",
    "@vercel/node": "^3.0.1",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.66.7",
    "cache-manager": "^7.2.0",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "express-rate-limit": "^8.0.1",
    "imagekit": "^6.0.0",
    "ioredis": "^5.7.0",
    "keyv": "^5.5.0",
    "lightningcss": "^1.31.1",
    "multer": "^2.0.2",
